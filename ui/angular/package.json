{
  "name": "cc-ui",
  "version": "1.0.0",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf _dist",
    "cp-build": "mkdir -p ../build/app && cp -vr _dist/* ../build/app && cp -v assets/* ../build/app",
    "builddev": "webpack --config dev.webpack.config.js",
    "build": "npm run clean && webpack --config prod.webpack.config.js && npm run cp-build",
    "start": "tsc && npm run clean && mkdir _dist && webpack-dev-server --progress --config dev.webpack.config.js",
    "minBuild": "webpack && uglifyjs ./_dist/bundle.js -c -m -o ./_dist/bundle.min.js --source-map ./_dist/bundle.min.js.map",
    "i18n": "ng-xi18n --i18nFormat=xlf2 --outFile=app/locale/messages.xliff2.xlf",
    "tsc:w": "tsc -w",
    "webpack:w": "webpack --config watch.webpack.config.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "4.4.4",
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/compiler-cli": "4.4.5",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/platform-server": "4.4.5",
    "@angular/router": "4.4.4",
    "angular2-highlight-js": "5.0.1",
    "chart.js": "2.8.0",
    "core-js": "2.4.1",
    "crypto-js": "3.1.9-1",
    "ng2-bs3-modal": "0.10.4",
    "ng2-charts": "1.6.0",
    "ngx-filter-pipe": "^2.1.2",
    "rxjs": "5.4.0",
    "systemjs": "0.20.12",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@types/angular": "1.6.32",
    "@types/node": "8.0.26",
    "angular-router-loader": "0.8.5",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-minify-booleans": "^6.9.4",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "2.1.1",
    "html-loader": "0.5.5",
    "style-loader": "0.23.1",
    "ts-loader": "5.3.3",
    "typescript": "3.0.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.17.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-middleware": "3.6.2"
  }
}
