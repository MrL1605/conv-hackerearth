{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "traceResolution": false,
    "removeComments": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "types": [
      "node"
    ],
    "allowJs": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "outDir": "build"
  }
}
